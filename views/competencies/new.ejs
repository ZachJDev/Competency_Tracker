<h1>This is the new competency form:</h1>
<h2>Competency #<%= count %></h2>
<form action="/competencies" method="POST">
  <div>
    <select name="topic">
      <option value="here!">Here!</option>
    </select>
  </div>
  <div>
    <label for="number">Competency Number:</label><input type="number"
    name="number" value = <%=count%> readonly>
  </div>
  <div>
    <label for="competencyName">Competency Name:</label
    ><input type="text" name="competencyName" placeholder="competency Name" />
  </div>
  <div>
    <label for="description">Competency Description:</label>
    <input
      type="text"
      name="description"
      placeholder="competency Description"
    />
  </div>
  <div>
    <button>Submit!</button>
  </div>
</form>
