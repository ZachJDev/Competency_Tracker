<div class="form-whole">
<h1 class="l-heading form-heading"> <%=competency.name%></h1>
<h2 class="m-heading form-title">Edit skill <%=skill.number%></h2>
<form action="/competencies/<%=competency._id%>/skills/<%= skill.id%>?_method=PUT" method=POST>
    <div class="form-group form-name">
      <label for="name">Skill:</label>
      <input autocomplete="off" type="text" name="name" value="<%= skill.name%>" />
    </div>
    <div class="button-group">
      <div class="form-group cancel form-button">
        <a href="/competencies/<%=competency._id%>"><p>Cancel</p></a>
      </div>
      <div class="form-group submit form-button">
        <button>Add</button>
      </div>
    </div>
</form>
</div>
</div>
<%- include('../partials/footer') %> 